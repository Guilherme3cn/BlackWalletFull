{
  "expo": {
    "name": "BlackVault Wallet",
    "slug": "blackvault-wallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "statusBar": {
        "backgroundColor": "#000000",
        "barStyle": "light-content"
      },
      "navigationBar": {
        "backgroundColor": "#000000",
        "barStyle": "light-content"
      },
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.guilherme3cn.blackvaultwallet"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-camera"
    ],
    "extra": {
      "eas": {
        "projectId": "42d236b0-0a04-419c-8765-a2c6b2d24ac9"
      }
    }
  }
}
